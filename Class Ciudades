package bibliotecadalaran;

/**
 *
 * @author Francesco
 */
public class Ciudades {

    private String nomCiu;
    private String lug;
    private int NumHab;
    private String raza;
    private String sub;

    public void setSub(String sub) {
        this.sub = sub;
    }

    public String getSub() {
        return sub;
    }

    

    public void setRaza(String raza) {
        this.raza = raza;
    }

    public String getRaza() {
        return raza;
    }

    public Ciudades() {
    }

    public void setNomCiu(String nomCiu) {
        this.nomCiu = nomCiu;
    }

    public void setLug(String lug) {
        this.lug = lug;
    }

    public void setNumHab(int NumHab) {
        this.NumHab = NumHab;
    }

    public String getNomCiu() {
        return nomCiu;
    }

    public String getLug() {
        return lug;
    }

    public int getNumHab() {
        return NumHab;
    }

    public Ciudades(String nomCiu, String lug, int NumHab, String raza, String sub) {
        this.nomCiu = nomCiu;
        this.lug = lug;
        this.NumHab = NumHab;
        this.raza = raza;
        this.sub = sub;
    }

    @Override
    public String toString() {
        return "Ciudades{" + "nomCiu = " + nomCiu + ", lug = " + lug + ", NumHab = " + NumHab + ", raza = " + raza +"subteranea = "+sub;
    }
    
    
}
